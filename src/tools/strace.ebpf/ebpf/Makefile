# src/tools/strace.ebpf/ebpf/Makefile -- Makefile for libebpf
#

TOP = ../../..

LIBRARY_NAME = ebpf

# eBPF sources
BIN_SRCS = \
	trace.h \
	trace_head.c \
	trace_tp_all.c \
	trace_kern_tmpl.c \
	trace_libc_tmpl.c \
	trace_file_tmpl.c \
	trace_fileat_tmpl.c \
	trace_clone_tmpl.c \
	trace_fork_tmpl.c \
	trace_vfork_tmpl.c \
	trace_fs_path_1_2_arg_tmpl.c \
	trace_fs_path_1_3_arg_tmpl.c \
	trace_fs_path_2_4_arg_tmpl.c \
	pid_check_ff_disabled_hook.c \
	pid_check_ff_fast_hook.c \
	pid_check_ff_full_hook.c \



BIN_OBJS = $(addsuffix .o,$(BIN_SRCS))

%.c.o: %.c Makefile
	$(LD) -r -b binary -o $@ $<

%.h.o: %.h Makefile
	$(LD) -r -b binary -o $@ $<

%.o: %.c %.h Makefile

OBJS = $(BIN_OBJS) \
	ebpf_file_set.o \


include ../../../Makefile.inc
